development:
  dialect: postgres
  database: <DBNAME>
  user: <DBUSER>
  password: <DBPASSWORD>
  host: 127.0.0.1
  pool: 5

test:
  url: {{envOr "TEST_DATABASE_URL" "postgres://postgres:postgres@127.0.0.1:5432/<DBNAME>_test"}}

production:
  url: {{envOr "DATABASE_URL" "postgres://postgres:postgres@127.0.0.1:5432/<DBNAME>_production"}}
